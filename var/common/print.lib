#!/bin/echo
# 定义颜色
BLUE_COLOR="\033[36m"
RED_COLOR="\033[31m"
GREEN_COLOR="\033[32m"
VIOLET_COLOR="\033[35m"
RES="\033[0m"


function delimiter_prefix(){
    echo ""
    base_delimiter
}

function delimiter_suffix(){
    base_delimiter
    echo ""
}

function delimiter(){
    echo ""
    base_delimiter
    echo ""
}

function base_delimiter(){
    echo -e "${BLUE_COLOR}=================分割符=================${RES}"
}
